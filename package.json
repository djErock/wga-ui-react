{
	"name": "ui",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"axios": "^0.19.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-scripts": "3.0.1"
	},
	"devDependencies": {
		"babel-eslint": "10.0.1",
		"eslint": "5.16.0",
		"eslint-config-airbnb": "^17.1.1",
		"eslint-plugin-filenames": "^1.3.2",
		"eslint-plugin-import": "^2.18.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.14.2",
		"lint-staged": "^9.1.0",
		"stylelint": "^10.1.0",
		"stylelint-config-standard": "^18.3.0"
	},
	"scripts": {
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"lint": "npm run lint:js && npm run lint:less",
		"lint:dir": "eslint",
		"lint:less": "stylelint \"**/*.less\" --fix",
		"lint:js": "eslint ./src --fix",
		"postcommit": "echo \"Pushing code to repo...\" && git push",
		"precommit": "echo \"Performing precommit...\" && npm run fetch-rebase && lint-staged",
		"scorch": "rm -rf node_modules && rm -rf package-lock.json && npm i && npm start",
		"start": "react-scripts start",
		"test": "react-scripts test"
	},
	"lint-staged": {
		"src/**/*.js": [
			"lint:js",
			"test:staged",
			"git add"
		],
		"src/**/*.test.js": [
			"lint:js",
			"test:staged",
			"git add"
		],
		"src/**/*.css": [
			"lint:css",
			"git add"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
